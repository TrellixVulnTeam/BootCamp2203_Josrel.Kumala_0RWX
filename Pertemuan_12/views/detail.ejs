<div class="container mt-3">
    <div class="row">
        <div class="col-md-8">
            <h2>Detail Contact</h2>
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                  <h5 class="card-title"><%= detail.nama %> </h5>
                  <h6 class="card-subtitle mb-2 text-muted"><%= detail.nomor %> </h6>
                  <p class="card-text"><%= detail.email %> </p>
                  <a href="/contact" class="card-link">&laquo; Kembali ke kontak</a>
                </div>
            </div>
        </div>
    </div>
</div>